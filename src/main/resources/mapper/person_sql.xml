<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="person_sql">
	<select id="select_one" resultType="person">
		SELECT * FROM Person
		<where>
			uniqueId = '${uniqueId}'
		</where>
	</select>
</mapper>